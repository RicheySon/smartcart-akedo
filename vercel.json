{
  "version": 2,
  "buildCommand": "sh -lc \"cd /vercel/path0/client && npm install --legacy-peer-deps --include=dev && npm run build\"",
  "outputDirectory": ".next",
  "installCommand": "sh /vercel/path0/vercel-install.sh",
  "framework": "nextjs",
  "functions": {
    "client/api/[...path].js": {
      "runtime": "nodejs18.x",
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}
