{
  "version": 2,
  "buildCommand": "npm --prefix client install --legacy-peer-deps && npm --prefix client run build",
  "outputDirectory": "client/.next",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": "nextjs",
  "functions": {
    "client/api/[...path].js": {
      "runtime": "nodejs18.x",
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}